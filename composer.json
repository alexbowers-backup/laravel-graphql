{
    "name": "alexbowers/laravel-graphql",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bowers",
            "email": "bowersbros@gmail.com"
        }
    ],
    "require": {
        "youshido/graphql": "v1.4.3.2",
        "laravel/framework": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "mockery/mockery": "^0.9.6",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "AlexBowers\\GraphQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexBowers\\GraphQL\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
